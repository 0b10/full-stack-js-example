{
  "name": "full-stack-js-example-server",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "dev": "nodemon ./bin/www | npx pino-pretty",
    "debug": "NODE_ENV=debug DEBUG=* nodemon -r pino-debug ./bin/www | npx pino-pretty -lc",
    "lint": "eslint ./",
    "refreshdb": "knex migrate:rollback --env test && knex migrate:latest --env test && knex seed:run --env test",
    "start": "node ./bin/www",
    "test": "NODE_ENV=test jest"
  },
  "dependencies": {
    "cookie-parser": "~1.4.3",
    "debug": "~2.6.9",
    "express": "~4.16.0",
    "express-pino-logger": "^4.0.0",
    "knex": "^0.16.5",
    "moment": "^2.24.0",
    "morgan": "~1.9.0",
    "pg": "^7.10.0",
    "pino": "^5.12.3"
  },
  "devDependencies": {
    "eslint": "^5.16.0",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-plugin-import": "^2.17.2",
    "jest": "^24.7.1",
    "nodemon": "^1.18.11",
    "pino-debug": "^1.2.0",
    "pino-pretty": "^2.6.1",
    "supertest": "^4.0.2"
  }
}
